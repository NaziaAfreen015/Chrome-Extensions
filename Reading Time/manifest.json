{
    "name": "Reading Time Extension",
    "description": "An extension that estimates the reading time of articles.",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "content_scripts": [ 
        {
            "matches": [
            
            "<all_urls>"
            ],
            "js": ["scripts/content.js"]
        }
    ]
}